{
  "manifest_version": 2,
  "name": "Text Manipulator",
  "version": "1.0",
  "description": "A browser extension that performs various text manipulation operations.",
  "icons": {
    "48": "images/icon48.png",
    "96": "images/icon96.png"
  },
  "browser_action": {
    "default_title": "Text Manipulator",
    "default_popup": "index.html"
  },
  "permissions": ["activeTab"]
}
